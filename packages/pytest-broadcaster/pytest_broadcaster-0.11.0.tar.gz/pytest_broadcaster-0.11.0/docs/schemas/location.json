{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://charbonnierg.github.io/pytest-broadcaster/latest/schemas/location.json",
  "title": "Location",
  "description": "Location in code source.",
  "type": "object",
  "required": [
    "filename",
    "lineno"
  ],
  "properties": {
    "filename": {
      "type": "string",
      "description": "File name."
    },
    "lineno": {
      "type": "integer",
      "description": "Line number."
    }
  }
}