from grok3.client import GrokClient

def main():
    # –°–æ–∑–¥–∞—ë–º –∫–ª–∏–µ–Ω—Ç (cookies –ø–æ–ª—É—á–∞—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏, –µ—Å–ª–∏ –∏—Ö –Ω–µ—Ç)
    client = GrokClient()
    
    # –°–æ–∑–¥–∞—ë–º –∑–∞–ø—Ä–æ—Å
    message = "–°–æ–∑–¥–∞–π –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –∫–æ—Å–º–∏—á–µ—Å–∫–æ–≥–æ –∫–æ—Ä–∞–±–ª—è"
    
    # –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∑–∞–ø—Ä–æ—Å
    result = client.ChatCompletion.create(message)
    print("–û—Ç–≤–µ—Ç Grok:", result.modelResponse.message)
    
    # –°–æ—Ö—Ä–∞–Ω—è–µ–º –ø–µ—Ä–≤–æ–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ, –µ—Å–ª–∏ –æ–Ω–æ –µ—Å—Ç—å
