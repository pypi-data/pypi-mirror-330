<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <soapenv:Header/>
    <soapenv:Body>
        <NotificaPagamentoDebitoResponse xmlns="http://schemi.informatica.maggioli.it/ws/pagopa/Notify"/>
        <CodiceIPA>{{codice_ipa}}</CodiceIPA>
        <IDOperazione>NotificaPagamentoDebito</IDOperazione>
        <DataRisposta>{{today}}</DataRisposta>
        <EsitoOperazione>OK</EsitoOperazione>
        <DatiDettaglioRisposta><![CDATA[
        <RispostaNotificaPagamentoDebito xmlns="http://schemi.informatica.maggioli.it/operations/jcgpagopa/notify/1_0">
            <Esito>
                <Esito>{{esito}}</Esito>
                <Messaggio>{{messaggio}}</Messaggio>
            </Esito>
        </RispostaNotificaPagamentoDebito>
        ]]></DatiDettaglioRisposta>
        <Messaggi>
            <Messaggio>
                <Codice>{{esito}}</Codice>
                <Descrizione>{{messaggio}}</Descrizione>
            </Messaggio>
        </Messaggi>
    </soapenv:Body>
</soapenv:Envelope>